  <!-- page wrapper starts -->
  {{>alertMessage}}
  <!-- <div class="pageWrapper landingPageWrapper"> -->
    <div class="container-fluid">
    <div class="row" id="shoeSearchArea">
      <div class="col-xs-12">
        <form class="form-horizontal shoeSearchForm" action="/alternateResult" method="get">
              <!-- {{>alertMessage}} -->
          <label>I Have the Brand:</label> <input type="text" name="have" class="shoeBrands" />
          <div class="control-group">
            <label class="control-label">In Size</label>
            <div class="controls">
            <select name="input_size">
              {{#each sizes}}
                  <option value={{size}} selected="selected">{{size}}</option>
              {{/each}}
            </select>
          </div>
          <label>I Want the Brand: </label> <input type="text" name="want" class="shoeBrands" />
          <input type="submit" id="shoeSearchSubmit" class="btn btn-info btn-lg" value="Size Me!">
          <p class="hyperlink"> Didn't find your brand? <a href="/addShoe" style="color: blue"> Recommend it to us here </a> </p>
        </form>
      </div>
    </div>
    </div>
