<body>
<!-- website wrapper starts -->
<div class="websiteWrapper landingWrapper"> 
  
  <!-- main menu wrapper starts -->
  {{> menu_logged_in}}
  <!-- header wrapper starts -->
  <div class="headerOuterWrapper">
    <div class="headerWrapper"> <a href="" class="mainMenuButton"><span>Menu</span></a></div>
  </div>
    <!-- page wrapper starts -->
  {{#if hist}}
    <div class="portfolioTwoPageWrapper">
    <div class="titleBox"> 
      <h1>Search History</h1>
    </div>
<!-- portfolio wrapper starts -->
    {{#each hist}}
      <div class="portfolioTwoItemWrapper">
        <!-- <img class="replayImage" src={{image_url}}> -->
        <a class="portfolioTwoItemImageWrapper shoeBrandExpand" href={{result_image_url}}>
          <img src={{result_image_url}} alt=""/>
        </a>
        <div class="favoritesTwoItemWrapper">
          <p class="portfolioTwoItemTitle">Your Size: {{size}}</p>
        </div>
      </div>
    {{/each}}
  </div>
  {{else}}
    <div class="pageWrapper faqPageWrapper"> 
      
      <!-- page content wrapper starts -->
      <div class="pageContentWrapper infoBox"> 
        <!-- page title starts -->
        <h1>No history yet!</h1>
        <!-- page title ends -->
        <p>What are you waiting for?</p>
        <a href="/search" class="searchButton"><h5>Start Searching Now!</h5></a>
      </div>
    </div>
  {{/if}}  
  <!-- footer wrapper starts -->
  <div class="footerWrapper"> 
  </div>
  <!-- page wrapper starts -->
<!--   <div class="portfolioTwoPageWrapper"> 
    <center><h3> Your Search History: </h3></center>
    {{#each hist}}
      <p>Your searched brand: <img class="replayImage" src={{original_image_url}}></p>
      <p> Brand you converted to: <img class="replayImage" src={{result_image_url}}></p>
      <p> Your converted size: {{size}}</p>
    {{/each}}  -->
</div>
